<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".Group_th">

    <ImageView
        android:id="@+id/th_tolife"
        android:layout_width="140dp"
        android:layout_height="137dp"
        android:src="@drawable/tolife1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.014"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.011" />


    <TextView
        android:id="@+id/group_thera"
        android:layout_width="174dp"
        android:layout_height="39dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="72dp"
        android:gravity="top"
        android:text="Group Therapy"
        android:textAppearance="@style/group_thera"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.731"
        app:layout_constraintStart_toEndOf="@+id/th_tolife"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView

        android:layout_width="487.46dp"
        android:layout_height="618.62dp"
        android:src="@drawable/bg_group_thereapy"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteX="-39dp" />

    <ImageView

        android:layout_width="287dp"
        android:layout_height="286.97dp"
        android:src="@drawable/group1"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.032"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/th_tolife"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView

        android:id="@+id/imageView2"
        android:layout_width="287dp"
        android:layout_height="286.97dp"
        android:src="@drawable/group2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="444dp"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <ImageView


        android:layout_width="169dp"
        android:layout_height="161dp"
        android:layout_marginBottom="552dp"
        android:src="@drawable/bg_2_group_therepy"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.876"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>